<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="nh" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AfficherCoursCategory">
   <stylesheets>
      <URL value="@style.css" />
   </stylesheets>
   <children>
      <ImageView pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bg2.png" />
         </image>
      </ImageView>
      <ListView fx:id="affichercategory" layoutX="483.0" layoutY="140.0" prefHeight="298.0" prefWidth="472.0" />
      <Button fx:id="supprimer" layoutX="547.0" layoutY="442.0" mnemonicParsing="false" onAction="#supprimercategory" text="Supprimer" />
      <Button fx:id="mod" layoutX="638.0" layoutY="442.0" mnemonicParsing="false" onAction="#mod" text="Modifier" />
      <Button fx:id="returnToAdd" layoutX="896.0" layoutY="100.0" mnemonicParsing="false" onAction="#returnToAdd" text="Ajouter" />
      <ImageView fx:id="fx_return" fitHeight="40.0" fitWidth="55.0" layoutX="28.0" layoutY="17.0" onDragDetected="#returnToLocation" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@back.png" />
         </image>
      </ImageView>


   </children>
</AnchorPane>
